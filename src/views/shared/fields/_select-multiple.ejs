<div class="row">
  <div class="col-7">
    <h5>
      <img src="<%= icon %>" id="<%= id %>-icon">
      <div
        id="<%= id %>-loader"
        class="spinner-border spinner-small spinner-light text-primary"
        style="display:none;"
        role="status"
      >
        <span class="sr-only">Loading...</span>
      </div>


      <%= title %>
      <span id="<%= id %>-selected" class="badge badge-secondary"></span>
    </h5>
  </div>
  <div class="col-5">
    <ul class="nav justify-content-end">
      <li class="nav-item">
        <a id="<%= id %>-select-all" class="nav-link active" href="#">Select all</a>
      </li>
    </ul>
  </div>
</div>
<div class="form-group">
  <select id="<%= id %>" multiple="multiple" class="form-control" size="1">
    <option value="">None</option>
  </select>
</div>
